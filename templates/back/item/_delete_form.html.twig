<form method="post" action="{{ path('app_back_item_delete', { id: item.id }) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
  <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ item.id) }}" />
  <button class="btn btn-danger btn-sm mx-1 mb-2 bi bi-trash"> Delete</button>
</form>
